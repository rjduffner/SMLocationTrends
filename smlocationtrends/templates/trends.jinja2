<!DOCTYPE html>
<html>
    <head>
        <title>Survey Monkey Location Trends</title>
        <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
        <meta charset="utf-8">
    </head>
    <body>
        <table>
            <tbody>
                {% for key, value in information.iteritems()  %}
                <tr>
                    <td>{{ key }}</td>
                    <td>{{ value }}</td>
                </tr>
                {% endfor %}
            </tbody>
        </table>

        {% set keys = respondents[0].keys() %}

        <table>
            <thead>
                <tr>
                     {% for key in keys %}
                     <td>{{ key }}</td>
                     {% endfor %}
                </tr>
            </thead>
                <tbody>
                    {% for respondent in respondents %}
                    <tr>
                        {% for key in keys %}
                        <td>{{ respondent[key] }}</td>
                        {% endfor %}
                    </tr>
                    {% endfor %}
                </tbody>
        </table>
        
    </body>
